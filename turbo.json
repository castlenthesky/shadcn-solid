{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": ["**/.env.*local"],
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".vercel/output/**", ".vinxi/**"],
			"env": ["COMPATIBILITY_DATE", "VITE_CJS_IGNORE_WARNING"]
		},
		"check": {
			"dependsOn": ["^check"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
